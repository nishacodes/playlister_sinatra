<div class="playlist white">
  <h4> Playlist </h4>
</div>

<div class="songlist white"> 
  <%= simple_partial :_page_title %>
  <ul class="list"> 
    <% Artist.all.each do |artist|%>
      <a href=<%="/artist/#{artist.name.gsub(' ','_')}"%>> 
        <li><%= artist.name %></li>
      </a>
    <% end %>
  </ul>
</div>